# Gemini AI Configuration
# احصل على مفتاح API من: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_api_key_here

# Supabase Configuration
# احصل على هذه القيم من: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Admin Credentials (Development Only - Change in Production!)
# For production, use proper authentication system
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=admin123

# TRTC Configuration (For Vercel Serverless Function)
# احصل على هذه القيم من: https://console.trtc.io
# ⚠️ هام: لا تضع TRTC_SECRET_KEY في .env المحلي
# استخدم Vercel Environment Variables فقط للـ SECRET_KEY
TRTC_SDK_APP_ID=20029772
TRTC_SECRET_KEY=your_trtc_secret_key_here
